{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base",
        "prettier"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "ignorePatterns": ["webpack.config.js", "./dist"],
    "rules": {
        "no-console": "off",
        "quotes": [
            "error",
            "double"
        ]
    }
}
